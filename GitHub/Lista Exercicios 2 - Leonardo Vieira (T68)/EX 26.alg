Algoritmo "exe 26"

Var
   cont,total,soma:inteiro
   media:real


Inicio
      cont<-1
   enquanto (cont<>0) faca
      escreval("digite sua idade")
      leia(cont)
      total<-total+1
      soma<-soma+cont
   fimenquanto
   total<-total-1
   media<-soma/total
   escreval(" A meida da idade é: ",media)


Fimalgoritmo